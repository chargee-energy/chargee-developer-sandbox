{
  "name": "chargee-developer-sandbox",
  "version": "0.1.0",
  "description": "React frontend demonstrating Ampere API integration with real-time energy monitoring",
  "keywords": ["react", "energy", "api", "chargee", "ampere", "real-time", "monitoring"],
  "author": "Chargee Energy",
  "license": "MIT",
  "homepage": "https://github.com/chargee/chargee-developer-sandbox",
  "repository": {
    "type": "git",
    "url": "https://github.com/chargee/chargee-developer-sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/chargee/chargee-developer-sandbox/issues"
  },
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t chargee-sandbox .",
    "docker:run": "docker run -p 3000:80 chargee-sandbox",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
